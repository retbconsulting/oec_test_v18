    <template id="custom_styled_portal_docs_entry" inherit_id="portal.portal_docs_entry">
        <xpath expr="//a" position="attributes">
            <attribute name="style">box-shadow: inset .5rem .5rem rgba(63, 0, 0, .8);</attribute>
        </xpath>
    </template>

    <template id="main_portal_my_home" inherit_id="portal.portal_my_home">
        <xpath expr="//div[@id='portal_client_category']" position="before">
            <t t-set="partner" t-value="request.env.user.partner_id"/>
        </xpath>
        <!--<xpath expr="//div[hasclass('o_portal_docs')]" position="before">
                <t t-set="portal_vendor_category_enable" t-value="True"/>
            </xpath>-->
        <xpath expr="//div[@id='portal_client_category']" position="attributes">
            <attribute name="style">display: none !important;</attribute>
        </xpath>
        <div id="portal_vendor_category" position="inside">
            <div class="row g-4">
                <t t-call="portal.portal_docs_entry">
                    <t t-set="icon" t-value="'/mandat_cotisation_management/static/src/img/profile.svg'"/>
                    <t t-set="title">Mon profil</t>
                    <t t-set="text">Consultez les informations de votre profil.</t>
                    <t t-set="url" t-value="'/my-profil'"/>
                    <t t-set="config_card" t-value="True"/>
                </t>
                <t t-call="portal.portal_docs_entry">
                    <t t-set="icon" t-value="'/mandat_cotisation_management/static/src/img/profile.svg'"/>
                    <t t-set="title">Inscription</t>
                    <t t-set="text">Veuillez compléter votre demande d’inscription ici.</t>
                    <t t-set="url" t-value="'/registration'"/>
                    <t t-set="config_card" t-value="True"/>
                </t>


                <t t-if="partner.status_register in ['registered']">
                    <t t-call="portal.portal_docs_entry">
                        <t t-set="icon" t-value="'/internship_management_module/static/description/cotisations.svg'"/>
                        <t t-set="title">Mes cotisations</t>
                        <t t-set="text">Consultez toutes vos cotisations en un seul endroit.</t>
                        <t t-set="url" t-value="'/cotisations'"/>
                        <t t-set="config_card" t-value="True"/>
                    </t>

                    <t t-call="portal.portal_docs_entry">
                        <t t-set="icon" t-value="'/internship_management_module/static/description/declarations.svg'"/>
                        <t t-set="title">Mes déclarations de mandats</t>
                        <t t-set="text">Consultez toutes vos déclarations de mandats en un seul endroit.</t>
                        <t t-set="url" t-value="'/cotisations'"/>
                        <t t-set="config_card" t-value="True"/>
                    </t>


                    <t t-call="portal.portal_docs_entry">
                        <t t-set="icon" t-value="'/internship_management_module/static/description/stag_enc.svg'"/>
                        <t t-set="title">Mes stagiaires encadrés</t>
                        <t t-set="text">Consultez tous les dossiers des stagiaires que vous avez encadré.</t>
                        <t t-set="url" t-value="'/my/internships/ms'"/>
                        <t t-set="config_card" t-value="True"/>
                    </t>


                    <t t-call="portal.portal_docs_entry">
                        <t t-set="icon" t-value="'/internship_management_module/static/description/stag_cont.svg'"/>
                        <t t-set="title">Mes stagiaires contrôlés</t>
                        <t t-set="text">Consultez tous les dossiers des stagiaires que vous avez contrôlé.</t>
                        <t t-set="url" t-value="'/my/internships/cs'"/>
                        <t t-set="config_card" t-value="True"/>
                    </t>
                </t>
                <t t-call="portal.portal_docs_entry">
                    <t t-set="icon" t-value="'/portal/static/src/img/portal-connection.svg'"/>
                    <t t-set="title">Connexion &amp; Sécurité</t>
                    <t t-set="text">Configurez vos paramètres de connexion</t>
                    <t t-set="url" t-value="'/my/security'"/>
                    <t t-set="config_card" t-value="True"/>
                </t>
            </div>
        </div>
        <xpath expr="//div[@id='portal_common_category']" position="replace"/>
    </template>
