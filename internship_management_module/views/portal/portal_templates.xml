<odoo>

    <template id="oec_custom_signup" inherit_id="auth_signup.signup">
        <xpath expr="//form" position="attributes">
            <attribute name="action">/demande-inscription</attribute>
        </xpath>
    </template>

    <!--    <template id="remove_docs_extension" inherit_id="portal_my_home_documents" priority="30">-->
    <!--        <xpath expr="." position="replace"/>-->
    <!--    </template>-->
    <template id="portal_my_details_custom" inherit_id="portal.portal_my_details">
        <xpath expr="//form" position="attributes">
            <attribute name="enctype">multipart/form-data</attribute>
        </xpath>
        <xpath expr="//div[hasclass('col-lg-8')]" position="before">
            <div class="o_website_calendar col-lg-8">
                <style>
                    .o_website_calendar {
                    /* Overall container styling */
                    background-color: #f0f0f0; /* Adjust background color as needed */
                    border: 1px solid #ccc;
                    border-radius: 5px;
                    padding: 20px;
                    }

                    .wizard {
                    display: flex;
                    justify-content: space-between;
                    list-style: none;
                    padding: 0;
                    margin: 0;
                    }

                    .wizard li {
                    flex: 1;
                    text-align: center;
                    padding: 10px;
                    border-bottom: 2px solid transparent; /* For active step */
                    }

                    .wizard li.text-primary {
                    /* Active step styling */
                    border-bottom-color: #007bff; /* Adjust color as needed */
                    color: #007bff;
                    font-weight: bold;
                    }

                    .chevron {
                    /* Style the chevron as needed */
                    display: inline-block;
                    margin-left: 5px;
                    /* Add appropriate font-family, font-size, and color */
                    }
                </style>

                <div class="oe_structure container mb128 o_website_calendar_appointment">
                    <ul class="wizard mt32 float-right">
                        <li t-attf-class="#{partner.status=='draft' and 'text-primary' or 'text-muted'}"
                            class="text-primary">Dossier en cours
                            <span class="chevron"/>
                        </li>
                        <li t-attf-class="#{partner.status=='submitted' and 'text-primary' or 'text-muted'}">Soumis
                            <span class="chevron"/>
                        </li>
                        <li t-attf-class="#{partner.status=='completed' and 'text-primary' or 'text-muted'}">Dossier
                            complet
                            <span class="chevron"/>
                        </li>
                        <li t-attf-class="#{partner.status=='incompleted' and 'text-primary' or 'text-muted'}">Dossier
                            incomplet
                            <span class="chevron"/>
                        </li>
                        <li t-attf-class="#{partner.status=='drop_off' and 'text-primary' or 'text-muted'}">Déposé
                            <span class="chevron"/>
                        </li>
                        <li t-attf-class="#{partner.status=='signed_contract' and 'text-primary' or 'text-muted'}">
                            Contrat signé
                            <span class="chevron"/>
                        </li>
                    </ul>
                </div>
            </div>
        </xpath>
        <xpath expr="//div[hasclass('col-lg-8')]/div[hasclass('clearfix')]" position="inside">
            <div class="clearfix">
                <button class="btn btn-primary float-left mb32">Enregistrer</button>
                <a href="/my/account/submit" class="btn btn-primary float-right mb32 ">Soumettre</a>
            </div>
        </xpath>
        <xpath expr="//div[hasclass('col-lg-8')]/div[hasclass('clearfix')]/button[@type='submit']" position="replace"/>
        <!--<xpath expr="//a[contains(@class, 'btn btn-secondary me-2') and contains(text(), 'Discard')]"
               position="replace"/>-->
        <xpath expr="//a[contains(@class, 'btn btn-light me-2') and contains(text(), 'Discard')]"
               position="replace"/>
    </template>

    <!-- Inherit the portal_my_details_fields template to add custom fields and remove unwanted fields -->
    <template id="portal_my_details_fields_custom" inherit_id="portal.portal_my_details_fields">
        <xpath expr="//label[@for='company_name']/parent::div" position="replace">
            <div></div>
        </xpath>
        <xpath expr="//div[@t-if='not partner_can_edit_vat' and @class='col-12 d-none d-xl-block']" position="replace">
            <div></div>

        </xpath>
        <xpath expr="//div[label[@for='name']]" position="after">
            <div t-attf-class="form-group #{error.get('second_name') and 'o_has_error' or ''} col-xl-6">
                <label class="col-form-label" for="second_name">Prénom</label>
                <input type="text" name="second_name"
                       t-attf-class="form-control #{error.get('second_name') and 'is-invalid' or ''}"
                       t-att-value="second_name or partner.second_name"
                       t-att-readonly="None if partner.status=='draft' else '1'"/>
            </div>
            <div t-attf-class="form-group #{error.get('place_of_birth') and 'o_has_error' or ''} col-xl-6">
                <label class="col-form-label" for="place_of_birth">Lieu de naissance</label>
                <input type="text" name="place_of_birth"
                       t-attf-class="form-control #{error.get('place_of_birth') and 'is-invalid' or ''}"
                       t-att-value="place_of_birth or partner.place_of_birth"
                       t-att-readonly="None if partner.status=='draft' else '1'"/>
            </div>
            <div t-attf-class="form-group #{error.get('birthday') and 'o_has_error' or ''} col-xl-6">
                <label class="col-form-label" for="birthday">Date de naissance</label>
                <input type="date" name="birthday"
                       t-attf-class="form-control #{error.get('birthday') and 'is-invalid' or ''}"
                       t-att-value="birthday or partner.birthday"
                       t-att-readonly="None if partner.status=='draft' else '1'"/>
            </div>
        </xpath>
        <xpath expr="//label[@for='state_id']/parent::div" position="after">
            <div t-attf-class="form-group #{error.get('internship_supervisor_id') and 'o_has_error' or ''} col-xl-6">
                <label class="col-form-label" for="internship_supervisor_id">Maître de stage</label>
                <select name="internship_supervisor_id"
                        t-attf-class="form-control #{error.get('internship_supervisor_id') and 'is-invalid' or ''}"
                        t-att-readonly="None if partner.status=='draft' else '1'">
                    <option value="">Maître de stage...</option>
                    <t t-foreach="supervisors or []" t-as="supervisor">
                        <option t-att-value="supervisor.id"
                                t-att-selected="supervisor.id == int(internship_supervisor_id) if internship_supervisor_id else supervisor.id == partner.internship_supervisor_id.id">
                            <t t-out="supervisor.name"/>
                        </option>
                    </t>
                </select>
            </div>
            <div t-attf-class="form-group #{error.get('internship_controler_id') and 'o_has_error' or ''} col-xl-6">
                <label class="col-form-label" for="internship_controler_id">Contrôleur de stage</label>
                <select name="internship_controler_id"
                        t-attf-class="form-control #{error.get('internship_controler_id') and 'is-invalid' or ''} #{partner.status=='draft' and '' or 'o_not_editable'}"
                        readonly="1">
                    <option value="">Contrôleur de stage...</option>
                    <t t-foreach="controlors or []" t-as="controlor">
                        <option t-att-value="controlor.id"
                                t-att-selected="controlor.id == int(internship_controler_id) if internship_controler_id else controlor.id == partner.internship_controler_id.id">
                            <t t-out="controlor.name"/>
                        </option>
                    </t>
                </select>
            </div>
            <div t-attf-class="form-group #{error.get('start_date') and 'o_has_error' or ''} col-xl-6">
                <label class="col-form-label" for="start_date">Date de début de stage</label>
                <input type="date" name="start_date"
                       t-attf-class="form-control #{error.get('start_date') and 'is-invalid' or ''}"
                       t-att-value="start_date or partner.start_date" readonly="1"/>
            </div>
            <div t-attf-class="form-group #{error.get('contract_date') and 'o_has_error' or ''} col-xl-6">
                <label class="col-form-label" for="contract_date">Date de contrat de stage</label>
                <input type="date" name="contract_date"
                       t-attf-class="form-control #{error.get('contract_date') and 'is-invalid' or ''}"
                       t-att-value="contract_date or partner.contract_date" readonly="1"/>
            </div>
            <div t-attf-class="form-group #{error.get('depot_date') and 'o_has_error' or ''} col-xl-6">
                <label class="col-form-label" for="depot_date">Date de dépôt de stage</label>
