                       t-attf-class="form-control #{error.get('depot_date') and 'is-invalid' or ''}"
                       t-att-value="depot_date or partner.depot_date" readonly="1"/>
            </div>
            <div t-attf-class="form-group col-xl-6" data-type="binary">
                <label class="col-form-label label-optional" for="iscae_certificate">Attestation originale d'inscription
                    ISCAE
                    <span t-if="partner.iscae_certificate" style="color:blue">(Document déjà chargé)</span>
                </label>
                <input type="file" name="iscae_certificate" class="file" data-show-upload="true"
                       data-show-caption="true" data-show-preview="true"
                       t-att-readonly="None if partner.status=='draft' else '1'"/>
            </div>
            <div t-attf-class="form-group col-xl-6" data-type="binary">
