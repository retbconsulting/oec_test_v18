    <template id="portal_my_home_cust">
        <t t-call="portal.portal_layout">

            <div>
                <h3 class="text-center">
                    <font style="color: rgb(57, 123, 33);">Rapports de stage</font>
                </h3>
            </div>
            <section
                    class="s_cover o_colored_level pt0 pb0 s_parallax_no_overflow_hidden undefined o_cc o_cc2 oe_img_bg"
                    data-scroll-background-ratio="0" data-snippet="s_cover" data-name="Cover"
                    style="position: relative; background-image: url(&quot;/my/home&quot;);">
                <div class="s_allow_columns o_container_small">
                    <p class="lead" style="text-align: center;">Consulter Vos Rapports</p>
                    <p style="text-align: center;">
                        <a href="/my/internships" class="btn btn-primary mb-2" data-original-title="" title=""
                           aria-describedby="tooltip961187">
                            <font style="font-size: 18px;">Consulter</font>
                        </a>
                    </p>
                </div>
            </section>
        </t>
    </template>


    <template id="portal_my_internships" name="My reports">
        <t t-call="portal.portal_layout">
            <t t-set="breadcrumbs_searchbar" t-value="True"/>

            <t t-call="portal.portal_searchbar">
                <t t-set="title">Rapports</t>
            </t>
            <t t-if="not reports">
                <p>There are currently no reports for your account.</p>
            </t>
            <t t-if="reports" t-call="portal.portal_table">
                <thead>
                    <tr class="active">
                        <th>#</th>
                        <th class="text-left">Thème</th>
                        <th class="text-left">Maître de stage</th>
                        <th class="text-left">Contrôleur de stage</th>
                        <th>Etat</th>
                        <th/>
                    </tr>
                </thead>
                <t t-foreach="reports" t-as="report">
                    <tr>
                        <td class="text-left">
                            <span t-field="report.report_id" class="badge badge-pill badge-success"/>
                        </td>
                        <td class="text-left">
                            <a t-attf-href="/my/report/#{slug(report)}">
                                <span t-field="report.name"/>
                            </a>
                        </td>
                        <td class="text-left">
                            <span t-field="report.internship_supervisor_id"/>
                        </td>
                        <td class="text-left">
                            <span t-field="report.internship_controler_id"/>
                        </td>
                        <td class="text-left">
                            <t t-if="report.signature">
                                <span class="badge badge-pill badge-info">Signé</span>
                            </t>
                            <t t-if="report.cs_decision">
                                <t t-if="report.cs_decision == 'valide'">
                                    <span class="badge badge-pill badge-success">Rapport validé</span>
                                </t>
                                <t t-if="report.cs_decision == 'reserve'">
                                    <span class="badge badge-pill badge-warning">Validé avec réserves</span>
                                </t>
                                <t t-if="report.cs_decision == 'rejet'">
                                    <span class="badge badge-pill badge-danger">Rapport rejeté</span>
                                </t>
                            </t>
                        </td>
                        <td class="text-center">
                            <a t-attf-href="/my/report/#{slug(report)}"
                               class="mb-2 btn btn-secondary rounded-circle btn-sm">
                                <t t-if="partner.id == report.internship_id.id">Joindre</t>
                                <t t-if="partner.id == report.internship_controler_id.id">Consulter</t>
                                <t t-if="partner.id == report.internship_supervisor_id.id">Consulter</t>
                            </a>
                        </td>
                    </tr>
                </t>
            </t>
        </t>
    </template>

